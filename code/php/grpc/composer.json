{
    "name": "cloudstreet/task-grpc",
    "description": "Task Management gRPC Service in PHP",
    "type": "project",
    "require": {
        "php": "^8.1",
        "grpc/grpc": "^1.57",
        "google/protobuf": "^3.25",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "grpc/protoc-gen-php-grpc": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "TaskGRPC\\": "src/",
            "Tasks\\V1\\": "generated/Tasks/V1/",
            "GPBMetadata\\": "generated/GPBMetadata/"
        }
    }
}